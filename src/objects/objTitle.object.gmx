        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Listen to game controller input - Step Event
with (objMain)
{
    wasAxis = false;
    if (abs(lastInputP1GamePad) &gt; 0.1 || (abs(lastInputP1GamePad) &gt; 0 &amp;&amp; abs(lastInputP1GamePad) &lt; 0.1)) {lastInputP1GamePadActual = lastInputP1GamePad;}
    if (abs(lastInputP1GamePad) &gt; 0.1 || (abs(lastInputP1GamePad) &gt; 0 &amp;&amp; abs(lastInputP1GamePad) &lt; 0.1)) {lastInputP2GamePadActual = lastInputP2GamePad;}
    
    lastInputP1GamePad = 0;
    lastInputP2GamePad = 0;
    
    for (i=gp_face1; i&lt;gp_axisrv; i++)
    {
        if gamepad_button_check(p1HasController, i)
        {
            lastInputP1GamePad = i;
            exit;
        }
    }
    
    for (i=gp_face1; i&lt;gp_axisrv; i++)
    {
        if gamepad_button_check(p2HasController, i)
        {
            lastInputP2GamePad = i;
            exit;
        }
    }
    
    if (abs(gamepad_axis_value(p1HasController,gp_axislh)) &gt; 0.5 || abs(gamepad_axis_value(p1HasController,gp_axislv)) &gt; 0.5)
    {lastInputP1GamePad = gamepad_axis_value(p1HasController,gp_axislv); wasAxis=true;}
    
    if (abs(gamepad_axis_value(p2HasController,gp_axislh)) &gt; 0.5 || abs(gamepad_axis_value(p2HasController,gp_axislv)) &gt; 0.5)
    {lastInputP2GamePad = gamepad_axis_value(p2HasController,gp_axislv); wasAxis=true;}
}
</string>
          </argument>
